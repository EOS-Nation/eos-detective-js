{
  "name": "eos-detective",
  "version": "0.1.0",
  "description": "EOS Detective",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/eosnationftw/eos-detective-js",
  "author": {
    "name": "EOS Nation",
    "url": "https://eosnation.io"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepare": "tsc",
    "lint": "tslint **/*.ts",
    "lint:specific": "tslint",
    "test": "jest",
    "docs": "typedoc --out docs src"
  },
  "husky": {
    "hooks": {
      "_comment": "Use --no-verify to bypass",
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.ts": [
        "yarn lint:specific"
      ]
    },
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.9",
    "dotenv": "^6.1.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.14.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^4.1.0"
  }
}
